      "email": "dev@heady.io",
      "homepage": "https://www.heady.io/"
    }
  ],
  "require": {
    "php": "^5.5 || ^5.6 || ^7.0 || ^7.1 || ^7.2 || ^7.3 || ^7.4 || ^8.0 || ^8.1",
    "ext-mongodb": "1.*",
    "simplesamlphp/composer-module-installer": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=3.70"
