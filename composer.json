  "license": "MIT",
  "keywords": [
    "simplesamlphp",
    "modules",
    "store",
    "mongodb"
  ],
  "authors": [
    {
      "name": "Prajyot Khandeparkar",
      "email": "prajyot@heady.io"
    },
    {
